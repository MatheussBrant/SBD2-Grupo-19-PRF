    "# Converte data para datetime\n",
    "df['data_dt'] = pd.to_datetime(df['data_inversa'], format='%Y-%m-%d', errors='coerce')\n",
    "df['ano'] = df['data_dt'].dt.year\n",
    "df['mes'] = df['data_dt'].dt.month\n",
    "df['ano_mes'] = df['data_dt'].dt.to_period('M')\n",
    "\n",
    "# Acidentes por ano\n",
    "acidentes_ano = df.groupby('ano')['id'].nunique().sort_index()\n",
    "\n",
    "# Visualiza√ß√£o\n",
    "fig, ax = plt.subplots(figsize=(14, 7))\n",
    "\n",
    "# Cores diferentes para per√≠odo pandemia vs p√≥s-pandemia\n",
    "cores = ['#e74c3c' if ano <= 2021 else '#3498db' for ano in acidentes_ano.index]\n",
    "bars = ax.bar(acidentes_ano.index, acidentes_ano.values, color=cores, alpha=0.8, edgecolor='black')\n",
    "\n",
    "# Adiciona valores nas barras\n",
    "for i, (ano, valor) in enumerate(acidentes_ano.items()):\n",
    "    ax.text(ano, valor + 1000, f'{valor:,}', ha='center', fontweight='bold')\n",
    "\n",
    "# Linha de tend√™ncia\n",
    "z = np.polyfit(acidentes_ano.index, acidentes_ano.values, 1)\n",
    "p = np.poly1d(z)\n",
    "ax.plot(acidentes_ano.index, p(acidentes_ano.index), \"--\", color='black', alpha=0.5, linewidth=2, label='Tend√™ncia')\n",
    "\n",
    "ax.set_title('Evolu√ß√£o de Acidentes por Ano (2020-2025)\\nüî¥ Pandemia | üîµ P√≥s-Pandemia', fontsize=14, fontweight='bold')\n",
    "ax.set_xlabel('Ano', fontsize=12)\n",
    "ax.set_ylabel('Quantidade de Acidentes', fontsize=12)\n",
    "ax.legend()\n",
    "ax.grid(True, alpha=0.3, axis='y')\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Estat√≠sticas comparativas\n",
    "print('\\n' + '='*80)\n",
    "print('COMPARATIVO: PANDEMIA vs P√ìS-PANDEMIA')\n",
    "print('='*80)\n",
    "pandemia_anos = [ano for ano in acidentes_ano.index if ano <= 2021]\n",
    "pos_pandemia_anos = [ano for ano in acidentes_ano.index if ano >= 2022]\n",
    "\n",
    "if len(pandemia_anos) > 0:\n",
    "    pandemia = acidentes_ano[pandemia_anos].mean()\n",
    "    print(f\"M√©dia anual durante pandemia (2020-2021): {pandemia:,.0f}\")\n",
    "\n",
    "if len(pos_pandemia_anos) > 0:\n",
    "    pos_pandemia = acidentes_ano[pos_pandemia_anos].mean()\n",
    "    print(f\"M√©dia anual p√≥s-pandemia (2022-2025): {pos_pandemia:,.0f}\")\n",
    "\n",
    "if len(pandemia_anos) > 0 and len(pos_pandemia_anos) > 0:\n",
    "    variacao = ((pos_pandemia - pandemia) / pandemia) * 100\n",
    "    print(f\"Varia√ß√£o: {variacao:+.1f}%\")\n",
    "print('='*80)"
