    "# Lista de arquivos CSV por ano\n",
    "anos = [2020, 2021, 2022, 2023, 2024, 2025]\n",
    "dataframes = []\n",
    "\n",
    "# Configura√ß√µes de leitura para diferentes formatos\n",
    "encodings = ['latin1', 'utf-8', 'ISO-8859-1']\n",
    "separators = [',', ';']\n",
    "\n",
    "# Carrega e consolida todos os anos\n",
    "for ano in anos:\n",
    "    arquivo = f'acidentes{ano}_todas_causas_tipos.csv'\n",
    "    loaded = False\n",
    "    \n",
    "    # Tenta diferentes configura√ß√µes\n",
    "    for encoding in encodings:\n",
    "        for sep in separators:\n",
    "            try:\n",
    "                df_ano = pd.read_csv(arquivo, sep=sep, encoding=encoding, low_memory=False, on_bad_lines='skip')\n",
    "                # Verifica se carregou com sucesso (mais de 3 colunas)\n",
    "                if len(df_ano.columns) > 3:\n",
    "                    print(f'‚úì {ano}: {len(df_ano):,} registros (encoding={encoding}, sep=\"{sep}\")')\n",
    "                    dataframes.append(df_ano)\n",
    "                    loaded = True\n",
    "                    break\n",
    "            except Exception as e:\n",
    "                continue\n",
    "        if loaded:\n",
    "            break\n",
    "    \n",
    "    if not loaded:\n",
    "        print(f'‚úó Erro ao carregar {ano}: arquivo n√£o encontrado ou formato inv√°lido')\n",
    "\n",
    "# Consolida todos os dataframes\n",
    "if len(dataframes) > 0:\n",
    "    df = pd.concat(dataframes, ignore_index=True)\n",
    "    print(f'\\nüìä Total consolidado: {len(df):,} registros ({len(df.columns)} colunas)')\n",
    "else:\n",
    "    print('\\n‚ùå ERRO: Nenhum arquivo foi carregado com sucesso!')\n",
    "    print('Verifique se os arquivos CSV est√£o na pasta correta.')"
